[alias]
  a = add
  all = add -A .

  c = commit
  ca = commit --all
  cam = commit --all -m
  caa = commit --all --amend
  caam = commit --all --amend -m
  cm = commit -m

  cl = clone

  co = checkout
  cob = checkout -b

  d = diff

  m = merge

  ps = push
  pl = pull

  rb = rebase
  rba = rebase --abort
  rbc = rebase --continue
  rbs = rebase --skip

  r = remote

  re = reset

  st = status
  sts = status -s

  ss = stash
  ssu = stash -u
